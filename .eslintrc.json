{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended","eslint-config-google","plugin:react/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi":"off",
        "require-jsdoc":"warn",
        "indent":["error",4],
        "max-len": ["error",150]
    }
}